<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Control Panel</title>
    @Styles.Render("~/CPanel/login")
    @Styles.Render("~/Content/kendo/css")

    @*<script src="@Url.Content("~/Scripts/cultures/kendo.culture.en-US.min.js")" type="text/javascript"></script>*@
    
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/angularjs")
    @Scripts.Render("~/bundles/placeholder")
    @Scripts.Render("~/bundles/kendo")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/bootstrap")
</head>
<body>
    <div class="container body-content" ng-app="logInApp">
        <div class="row">
            <div class="col-md-3">
                <h4>Administrative Tools</h4>
                <ul>
                    <li>Users</li>
                    <li>Items</li>
                    <li>Sign Out</li>
                </ul>
            </div>
            <div class="col-md-9">
                @RenderBody()        
            </div>
        </div>
        
    </div>

    @RenderSection("scripts", required: false)
</body>
</html>
